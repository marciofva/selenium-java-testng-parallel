<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="SigIn suite" >
  	<test name="Sign in validation in Chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.hellofresh.challenge.test.SignInTest" />
		</classes>
	</test>
    	<test name="Sign in validation in Firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.hellofresh.challenge.test.SignInTest" />
		</classes>
	</test>
</suite>